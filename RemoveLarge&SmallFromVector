//Code written by Panha Sim

#include <iostream>
#include <vector>

using namespace std;

int main()
{
    
   int lindex, sindex;
   vector<int> v;
   srand(time(NULL));
   int largest = 0;
   int smallest = 110;
   
    for (int i=0;i<10;i++){
        v.push_back(rand()%100+1);
    }
    
    
    for (int i=0;i<v.size();i++){
       cout<<v[i]<<"    ";
   }
   
   
   for (int i=0;i<10;i++){
       if (v[i]>largest){
           largest = v[i];
           lindex = i;
       }
   }
    v.erase(v.begin()+lindex);
   for(int i=0; i<10;i++){
        if(v[i]<smallest){
           smallest = v[i];
           sindex = i;
       }
   }
   v.erase(v.begin() + sindex);
  
   
  cout<<endl;
  cout<<"------------------------------"<<endl;


  for (int i=0;i<v.size();i++){
      cout<<v[i]<<"    ";
  }
}

//Code written by Panha Sim
